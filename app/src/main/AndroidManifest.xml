<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fblaapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fbla_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fbla_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TermsAndConditions" />
        <activity android:name=".FrequentlyAskedQuestions" />
        <activity android:name=".EventFormSubmitted" />
        <activity android:name=".CurrentEventForm" />
        <activity android:name=".JoinFormFinish" />
        <activity android:name=".JoinForm" />
        <activity android:name=".CurrentEvents" />
        <activity android:name=".ViewAttendance" />
        <activity android:name=".AddAttendance" />
        <activity android:name=".AboutUs" />
        <activity android:name=".Attendance" />
        <activity android:name=".Officers" />
        <activity android:name=".SocialMediaLinks" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>